  </tr>
 {{#each quizlist}}
 {{!-- <tr>
  <td><a href="/quiz/{{id}}">{{quiz_name}}</a></td> --}}
 <tr id={{id}}>
    <td><a href="/quiz/{{id}}" data-quizid={{id}}  onclick="updateQuiz({{id}})">{{quiz_name}}</a></td>
