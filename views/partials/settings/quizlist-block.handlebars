    </tr>
{{/each}}
</table>
<button onclick="addQuiz()" id="addquiz-button" class="dashboard-button">+Add Course</button>


    <script>

function updateQuiz(id){
    console.log('Display Add Quiz');
        $.get( '/addquiz/'+id, function( data ) {
            $( '#display-stage' ).html( data );
          });
    }

function addQuiz(){
    console.log('Display Add Quiz');
        $.get( '/addquiz/', function( data ) {
            $( '#display-stage' ).html( data );
          });
    }

function deleteQuiz(id){
console.log("Deleteing Quiz "+id);
$.ajax({
    url: '/api/delete/'+id,
    type: 'DELETE',
    success: function(result) {
        $("#"+id).remove();
    }
});

}


</script>